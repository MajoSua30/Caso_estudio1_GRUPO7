@model char[,]

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Resultado</title>
</head>
<body>
    <h1>@ViewBag.Resultado</h1>
    <table>
        @for (int i = 0; i < 3; i++)
        {
            <tr>
                @for (int j = 0; j < 3; j++)
                {
                    <td>@Model[i, j]</td>
                }
            </tr>
        }
    </table>
</body>
</html>
